[defn get [path]
      "Return the context for a SSG project, located at PATH"
      :export
      @[ :path path
         :title "Nujel.net"
         :author "Ben"
         :content-root-dir "content"
         :deploy-dir "deploy"
         :theme "theme/default"
         :resources-needed @[]
         :loaders @[]]]
